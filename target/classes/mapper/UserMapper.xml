<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.ssm.demo.Dao.UserDao">
    <select id="selectByUserName" parameterType="java.lang.String" resultType="org.ssm.demo.Dto.UserDto">
        select * from user_t where user_name=#{user_name}
    </select>

</mapper>